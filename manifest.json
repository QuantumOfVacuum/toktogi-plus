{
   "author": "Brad McDermott",
   "background": {
      "scripts": [ "js/background-build.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "images/19.png",
         "38": "images/38.png"
      },
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "all_frames": false,
      "css": [ "css/inject.css" ],
      "js": [ "js/vendor/jquery-2.1.4.min.js", "js/browser_tools.js", "js/inject.js" ],
      "matches": [ "http://*/*", "https://*/*", "file:///*" ],
      "run_at": "document_end"
   } ],
   "description": "Popup Korean-English dictionary that shows you the definitions of Korean words when you hover the mouse next to them",
   "icons": {
      "128": "images/128.png",
      "16": "images/16.png",
      "48": "images/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "33",
   "name": "Toktogi: A Korean-English Popup Dictionary",
   "options_page": "options.html",
   "permissions": [ "http://*/*", "https://*/*", "unlimitedStorage", "tabs" ],
   "short_name": "Toktogi",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.6.6",
   "web_accessible_resources": [ "images/unlock.png", "images/lock.png", "images/plus.png" ]
}
